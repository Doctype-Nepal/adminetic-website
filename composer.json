{
    "name": "adminetic/website",
    "description": "",
    "keywords": [
        "adminetic",
        "website"
    ],
    "homepage": "https://github.com/pratiksh404/admin.website",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Pratik Shrestha",
            "email": "pratikdai404@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "pratiksh/nepalidate": "^1.4",
        "rappasoft/laravel-livewire-tables": "^2.12",
        "spatie/laravel-sitemap": "^6.3",
        "spatie/schema-org": "^3.14"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Adminetic\\Website\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adminetic\\Website\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Adminetic\\Website\\Provider\\WebsiteServiceProvider"
            ]
        }
    }
}